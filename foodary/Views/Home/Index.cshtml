
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
    <title>Password protection</title>
</head>
<body>
    <SCRIPT LANGUAGE="JavaScript">
        var testV = true;
        function password() {
          
            var pass1 = prompt('Please enter the password:', '');
            while (testV) {
                if (!pass1)
                    history.go(-1);
                if (pass1 == "tp7") {
                    alert('correct');
                    testV = false;
                    window.location.href = "Home/Home";
                    break;
                }
                
                var pass1 =
                    prompt('incorrect,please try again');
            }
            if (pass1 != "password" & testV == 999)
                history.go(-1);
            return " ";
        }
        document.write(password());
    </SCRIPT>
</body>
</html>
